{
    "presets": [["env", {
        "targets": {
            "browsers": "> 2% in US",
            "node": "current"
        }
    }], "react"],
    "plugins": [
        ["module-resolver", {
            "root": ["."],
            "alias": {
                "react": "preact-compat",
                "react-dom": "preact-compat"
            }
        }]
    ],
    "env": {
        "development": {
            "plugins": ["flow-react-proptypes"],
        }
    }
}
