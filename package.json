{
  "name": "mongaku",
  "version": "0.0.1",
  "description": "An image similarity search engine and database.",
  "main": "build/mongaku.js",
  "bin": {
    "mongaku": "build/bin/mongaku.js"
  },
  "scripts": {
    "build": "./src/bin/mongaku.js build",
    "build-watch": "./src/bin/mongaku.js build-watch",
    "client:prod": "NODE_ENV=production webpack -p",
    "lint": "eslint src && npm run flow",
    "postinstall": "npm run build",
    "pretest": "cp -R src/tests/data/ build/tests/data/",
    "test": "npm run lint && NODE_ENV=test tap --cov build/tests/*/*.js",
    "start": "./build/bin/mongaku.js start",
    "stop": "./build/bin/mongaku.js stop",
    "restart": "./build/bin/mongaku.js restart",
    "dev": "./build/bin/mongaku.js dev",
    "dev-test": "NODE_ENV=test supervisor -e js,jsx -w build -i node_modules,locales build/tests/server.js",
    "extract-i18n-js": "babel-jsxgettext build/*.js build/*/*.js build/*/*/*.js build/*/*/*/*.js translations/messages.po",
    "extract-i18n": "npm run extract-i18n-js && git add translations/messages.pot",
    "update-i18n": "tx pull -a && po2json.js -p translations/de/messages.po > translations/de/messages.json && po2json.js -p translations/it/messages.po > translations/it/messages.json",
    "migrate": "migrate",
    "coverage": "NODE_ENV=test tap --cov --coverage-report=lcov build/tests/*/*.js",
    "test-file": "NODE_ENV=test tap",
    "flow": "flow",
    "prettier": "prettier --write --no-bracket-spacing --trailing-comma=es5 --tab-width=4 --parser=babylon \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mongaku/mongaku.git"
  },
  "author": "John Resig <jeresig@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mongaku/mongaku/issues"
  },
  "homepage": "https://github.com/mongaku/mongaku",
  "dependencies": {
    "async": "2.6.0",
    "bcrypt": "1.0.3",
    "body-parser": "1.18.2",
    "concat-stream": "1.6.0",
    "connect-mongo": "2.0.0",
    "cookie-parser": "1.4.3",
    "csurf": "1.9.0",
    "csv-streamify": "3.0.4",
    "dotenv": "4.0.0",
    "elasticsearch-sanitize": "2.0.0",
    "express": "4.16.2",
    "express-session": "1.15.6",
    "farmhash": "2.0.4",
    "formidable": "1.1.1",
    "glob": "7.1.2",
    "gm": "1.23.0",
    "iconv-lite": "0.4.19",
    "imageinfo": "1.0.4",
    "intl": "1.2.5",
    "jsondiffpatch": "0.2.5",
    "jsonlint": "1.6.2",
    "kerberos": "0.0.23",
    "libxmljs": "0.18.7",
    "lodash.clonedeepwith": "4.5.0",
    "method-override": "2.3.10",
    "minimist": "1.2.0",
    "mongoosastic": "4.3.0",
    "mongoose": "4.13.7",
    "mongoose-version": "1.1.0",
    "morgan": "1.9.0",
    "naught": "1.6.0",
    "parse-dimensions": "0.0.8",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "password-generator": "2.2.0",
    "pastec": "0.0.5",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "readline-sync": "1.4.7",
    "request": "2.83.0",
    "serve-favicon": "2.4.5",
    "serve-static": "1.13.1",
    "shelljs": "0.7.8",
    "unzip2": "0.2.5",
    "valid-url": "1.0.9",
    "whatwg-fetch": "2.0.3",
    "yearrange": "0.0.5"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-jsxgettext": "0.5.0",
    "babel-loader": "7.1.2",
    "babel-plugin-flow-react-proptypes": "12.1.0",
    "babel-plugin-module-resolver": "3.0.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "eslint": "4.13.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.40.1",
    "eslint-plugin-react": "7.5.1",
    "exports-loader": "0.6.4",
    "flow-bin": "0.61.0",
    "migrate": "1.0.0",
    "mock-fs": "4.4.2",
    "mongo-in-memory": "0.0.5",
    "null-loader": "0.1.1",
    "phantom-pool": "1.2.2",
    "pre-commit": "1.2.2",
    "prettier": "1.9.2",
    "sauce-connect-launcher": "1.2.3",
    "sinon": "4.1.3",
    "supervisor": "0.12.0",
    "tap": "11.0.0",
    "webdriverio": "4.9.11",
    "webpack": "3.10.0"
  },
  "pre-commit": [
    "build",
    "lint",
    "extract-i18n"
  ],
  "nyc": {
    "exclude": [
      "src/tests",
      "src/server/cron.js",
      "src/converters"
    ],
    "extension": [
      ".jsx"
    ]
  }
}
