{
  "name": "mongaku",
  "version": "0.0.1",
  "description": "An image similarity search engine and database.",
  "main": "build/mongaku.js",
  "bin": {
    "mongaku": "build/bin/mongaku.js"
  },
  "scripts": {
    "build": "babel src --out-dir build && npm run client:dev",
    "client:dev": "webpack",
    "client:prod": "NODE_ENV=production webpack -p",
    "lint": "eslint src && npm run flow",
    "postinstall": "npm run build",
    "test": "npm run lint && NODE_ENV=test tap --cov build/tests/*/*.js",
    "dev": "supervisor -e js,jsx -w build -i node_modules,locales -- build/mongaku.js",
    "dev-test": "NODE_ENV=test supervisor -e js,jsx -w build -i node_modules,locales build/tests/server.js",
    "extract-i18n-js": "babel-jsxgettext build/*.js build/*/*.js build/*/*/*.js build/*/*/*/*.js translations/messages.po",
    "extract-i18n": "npm run extract-i18n-js && git add translations/messages.pot",
    "update-i18n": "tx pull -a && po2json.js -p translations/de/messages.po > translations/de/messages.json && po2json.js -p translations/it/messages.po > translations/it/messages.json",
    "migrate": "migrate",
    "coverage": "NODE_ENV=test tap --cov --coverage-report=lcov build/tests/*/*.js",
    "test-file": "NODE_ENV=test tap --cov",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mongaku/mongaku.git"
  },
  "author": "John Resig <jeresig@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mongaku/mongaku/issues"
  },
  "homepage": "https://github.com/mongaku/mongaku",
  "dependencies": {
    "async": "2.1.4",
    "bcrypt": "1.0.2",
    "body-parser": "1.16.0",
    "concat-stream": "1.6.0",
    "connect-mongo": "1.3.2",
    "cookie-parser": "1.4.3",
    "csurf": "1.9.0",
    "csv-streamify": "3.0.4",
    "dotenv": "4.0.0",
    "elasticsearch-sanitize": "2.0.0",
    "express": "4.14.0",
    "express-session": "1.14.2",
    "farmhash": "1.2.1",
    "formidable": "1.1.1",
    "gm": "1.23.0",
    "i18n-abide": "0.0.25",
    "iconv-lite": "0.4.15",
    "imageinfo": "1.0.4",
    "jsondiffpatch": "0.2.4",
    "jsonlint": "1.6.2",
    "kerberos": "0.0.22",
    "libxmljs": "0.18.4",
    "lodash.clonedeepwith": "4.5.0",
    "method-override": "2.3.7",
    "minimist": "1.2.0",
    "moment": "2.17.1",
    "mongoosastic": "4.2.4",
    "mongoose": "4.7.7",
    "mongoose-version": "1.1.0",
    "morgan": "1.7.0",
    "naught": "1.6.0",
    "parse-dimensions": "0.0.7",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "password-generator": "2.1.0",
    "pastec": "0.0.5",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "readline-sync": "1.4.5",
    "request": "2.79.0",
    "serve-favicon": "2.3.2",
    "serve-static": "1.11.1",
    "shelljs": "0.7.6",
    "unzip2": "0.2.5",
    "valid-url": "1.0.9",
    "yearrange": "0.0.5"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-eslint": "7.1.1",
    "babel-jsxgettext": "0.3.0",
    "babel-loader": "6.3.2",
    "babel-plugin-flow-react-proptypes": "0.20.0",
    "babel-preset-env": "1.1.8",
    "babel-preset-react": "6.16.0",
    "eslint": "3.13.1",
    "eslint-plugin-flowtype": "2.30.0",
    "eslint-plugin-react": "6.9.0",
    "flow-bin": "0.38.0",
    "migrate": "0.2.3",
    "mock-fs": "4.0.0-beta.1",
    "pre-commit": "1.2.2",
    "sinon": "1.17.7",
    "supervisor": "0.12.0",
    "tap": "9.0.3",
    "webpack": "2.2.1"
  },
  "pre-commit": [
    "build",
    "lint",
    "extract-i18n"
  ],
  "nyc": {
    "exclude": [
      "tests",
      "server/cron.js",
      "converters"
    ],
    "extension": [
      ".jsx"
    ]
  }
}
